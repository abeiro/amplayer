{
   "app": {
      "background": {
         "scripts": [ "background.js" ]
      }
   },
   "description": "Ampache HTML5 Player",
   "icons": {
      "128": "logo.png",
      "16": "logo16.png"
   },
   "manifest_version": 2,
   "name": "AMPlayer",
   "permissions": [ "webview", "http://*/", "notifications", "storage" ,"https://www.google-analytics.com/","fullscreen","identity","<all_urls>","unlimitedStorage"],
   "content_security_policy": "img-src 'self' * http://*/ https://*/",
    
   "version": "0.4.24"
}
