<?xml version='1.0' encoding='utf-8'?>
<widget id="com.orphan.amplayer" version="0.4.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>AMPlayer</name>
    <description>
        Ampache Player.
    </description>
    <author email="augusto.beiro@gmail.com" >
        Augusto Beiro
    </author>
    <access origin="*" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
	<icon src="img/logo.png" gap:platform="android" gap:density="ldpi" />
	<icon src="img/logo.png" gap:platform="android" gap:density="mdpi" />
	<icon src="img/logo.png" gap:platform="android" gap:density="hdpi" />
	<icon src="img/logo.png" gap:platform="android" gap:density="xhdpi" />
</widget>
